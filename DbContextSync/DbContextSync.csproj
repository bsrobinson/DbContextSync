<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>

    <Title>DbContextSync</Title>
    <Authors>Ben Robinson</Authors>
    <Description>Synchronise your database with your Entity Framework (EF) C# DBContext class files.</Description>

    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
     <PackageReference Include="Glob.cs" Version="5.1.1253" /> 
     <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.7.0" /> 
     <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
     <PackageReference Include="MySql.Data" Version="8.2.0" />
     <PackageReference Include="PastelEx" Version="1.0.4" />
     <PackageReference Include="CommandLineParser" Version="2.9.1" />
  </ItemGroup>

</Project>